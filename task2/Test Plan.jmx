        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler">
          <stringProp name="BeanShellSampler.query">try {
    String emailBody = vars.get(&quot;emailBody&quot;);
    String response = &quot;&quot;;
    boolean found = false;

    while (true) {
        String message = vars.get(&quot;message&quot;);
        String expectedResponse = vars.get(&quot;expected_response&quot;);
        
        if (message == null || expectedResponse == null) {
            break;
        }
        
        if (emailBody.contains(message)) {
            response = expectedResponse;
            found = true;
            break;
        }
    }

    if (found) {
        vars.put(&quot;responseMessage&quot;, response);
    } else {
        vars.put(&quot;responseMessage&quot;, &quot;&quot;);
    }
} catch (Exception e) {
    vars.put(&quot;errorMessage&quot;, e.getMessage());
}
</stringProp>
        </BeanShellSampler>
