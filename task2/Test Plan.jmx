            <stringProp name="script">def emailBody = vars.get(&apos;email_body&apos;)

def responses = [:]

def csvFileName = vars.get(&apos;__BeanShell(&apos;CSV Data Set Config.FileName&apos;)&apos;)
def csvReader = new BufferedReader(new FileReader(csvFileName))

def line
while ((line = csvReader.readLine()) != null) {
    def parts = line.split(&apos;,&apos;)
    if (parts.length == 2) {
        def message = parts[0].trim()
        def expectedResponse = parts[1].trim()
        responses[message] = expectedResponse
    }
}
csvReader.close()

def responseText = &apos;Неизвестный запрос&apos;
responses.each { key, value -&gt;
    if (emailBody.contains(key)) {
        responseText = value
    }
}

vars.put(&apos;response_text&apos;, responseText)

