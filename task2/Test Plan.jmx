        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="LoginAndSendEmail">
          <stringProp name="WebDriverSampler.script">from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.chrome.service import Service
from webdriver_manager.chrome import ChromeDriverManager
import time
import csv

def load_responses(filename):
    responses = {}
    with open(filename, mode=&apos;r&apos;, encoding=&apos;utf-8&apos;) as file:
        reader = csv.DictReader(file)
        for row in reader:
            message = row[&apos;message&apos;].strip()
            response = row[&apos;expected_response&apos;].strip()
            responses[message] = response
    return responses

def get_response_text(email_body, responses):
    for key, value in responses.items():
        if key in email_body:
            return value
    return &quot;Неизвестный запрос&quot;

driver = webdriver.Chrome(service=Service(ChromeDriverManager().install()))

driver.get(&quot;https://mail.yandex.ru&quot;)

login = driver.find_element(By.NAME, &quot;login&quot;)
login.send_keys(&quot;user2@yandex.ru&quot;)
login.send_keys(Keys.RETURN)

time.sleep(2)  

password = driver.find_element(By.NAME, &quot;passwd&quot;)
password.send_keys(&quot;pass2&quot;)
password.send_keys(Keys.RETURN)

time.sleep(5)  

inbox = driver.find_element(By.XPATH, &quot;//a[@href=&apos;/inbox/&apos;]&quot;)
inbox.click()

time.sleep(5)  

emails = driver.find_elements(By.XPATH, &quot;//div[contains(@class, &apos;mail-MessageSnippet&apos;)]&quot;)

responses = load_responses(&apos;responses.csv&apos;)

if emails:
    first_email = emails[0]
    first_email.click()

    time.sleep(3)  

    email_body = driver.find_element(By.XPATH, &quot;//div[@class=&apos;mail-Message-Body&apos;]&quot;).text
    print(&quot;Текст письма:&quot;, email_body)

    response_text = get_response_text(email_body, responses)

    reply_button = driver.find_element(By.XPATH, &quot;//button[@data-name=&apos;reply&apos;]&quot;)
    reply_button.click()
    
    time.sleep(2) 

    response_text_area = driver.find_element(By.XPATH, &quot;//textarea[@name=&apos;body&apos;]&quot;)
    response_text_area.send_keys(response_text)
    send_button = driver.find_element(By.XPATH, &quot;//button[@data-name=&apos;send&apos;]&quot;)
    send_button.click()

driver.quit()
</stringProp>
